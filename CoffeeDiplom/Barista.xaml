<Window x:Class="CoffeeDiplom.Barista"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CoffeeDiplom"
        mc:Ignorable="d"
        Title="Заказы" Height="500" Width="850" Icon="c.jpg">
    <Grid>
       
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="120"/>
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
      
        <StackPanel  Grid.Column="0" Orientation="Vertical">
            <Button Content="Назад"  Width="100"  Height="30" Margin="0,10,0,10"  Click="Nazad" Background="red"/>
            <Button Content="Coffee" Width="100"  Height="30" Margin="0,10,0,10" x:Name="Cofffe" Click="Coffe"/>
            <Button Content="Заказы" Width="100"  Height="30" Margin="0,10,0,10" Click="Zakaz"/>
        </StackPanel>

        <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden"  Grid.Column="1"></Frame>
    </Grid>
</Window>
